    <array>
        <dict>
            <key>CFBundleTypeName</key>        <string>Pineapple Notebook</string>
            <key>CFBundleTypeRole</key>        <string>Editor</string>
            <key>CFBundleTypeExtensions</key>
                                                <array>
                                                    <string>ipynb</string>
                                                </array>
            <key>CFBundleTypeIconFile</key>    <string>Pineapple-Doc.icns</string>
            <key>LSIsAppleDefaultForType</key> <true/>
            <key>LSTypeIsPackage</key>         <false/>
        </dict>
    </array>
